{
  "name": "frrri",
  "version": "0.0.1",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bitflut/frrri.git"
  },
  "author": "Marian Pl√∂sch",
  "contributors": [
    {
      "name": "Patrick Thonhauser"
    }
  ],
  "bugs": {
    "url": "https://github.com/bitflut/frrri/issues"
  },
  "homepage": "https://bitflut.gitbook.io/frrri/",
  "packageScope": "@ng-frrri/",
  "packages": [
    "libs/ngxs"
  ],
  "scripts": {
    "nx": "nx",
    "start": "run-p -r start:ng-integration json-server",
    "start:ng-integration": "nx serve ng-integration",
    "json-server": "json-server db.json",
    "test": "nx test",
    "lint": "nx workspace-lint && nx lint",
    "e2e": "nx e2e",
    "build:libs": "rm -rf dist && run-s build:libs:ngxs",
    "build:libs:ngxs": "ng-packagr -p libs/ngxs/package.json",
    "publish:libs": "run-s publish:libs:ngxs",
    "publish:libs:ngxs": "npm publish dist/@ng-frrri/-ngxs --access=public",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e:nx": "nx affected:e2e",
    "affected:e2e": "run-p -r json-server affected:e2e:nx",
    "affected:test": "run-p -r json-server affected:test:nx",
    "affected:test:nx": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.1.0",
    "@angular/common": "^9.1.0",
    "@angular/compiler": "^9.1.0",
    "@angular/core": "^9.1.0",
    "@angular/forms": "^9.1.0",
    "@angular/platform-browser": "^9.1.0",
    "@angular/platform-browser-dynamic": "^9.1.0",
    "@angular/router": "^9.1.0",
    "core-js": "^2.5.4",
    "rxjs": "~6.5.4",
    "zone.js": "^0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.901.0",
    "@angular/compiler-cli": "^9.1.0",
    "@angular/language-service": "^9.1.0",
    "@ngxs-labs/data": "^3.0.4",
    "@ngxs/devtools-plugin": "^3.6.2",
    "@ngxs/store": "^3.6.2",
    "@nrwl/angular": "^9.2.4",
    "@nrwl/cypress": "9.2.4",
    "@nrwl/jest": "9.2.4",
    "@nrwl/workspace": "9.2.4",
    "@types/jest": "25.1.4",
    "@types/node": "~8.9.4",
    "codelyzer": "~5.0.1",
    "cypress": "^4.1.0",
    "dotenv": "6.2.0",
    "eslint": "6.8.0",
    "jest": "25.2.3",
    "jest-preset-angular": "8.1.2",
    "json-server": "^0.16.1",
    "lodash": "^4.17.15",
    "ng-mocks": "^9.2.0",
    "ng-packagr": "^9.1.3",
    "npm-run-all": "^4.1.5",
    "parse-link-header": "^1.0.1",
    "prettier": "1.19.1",
    "ts-jest": "25.2.1",
    "ts-node": "~7.0.0",
    "tslint": "~6.0.0",
    "typescript": "~3.8.3"
  }
}