{
  "$schema": "../../node_modules/ng-packagr/package.schema.json",
  "name": "@ng-frrri/ngxs",
  "version": "0.0.0",
  "license": "MIT",
  "peerDependencies": {
    "@angular/common": "^9.1.0",
    "@angular/core": "^9.1.0",
    "@ngxs/store": "^3.6.2",
    "@ngxs-labs/data": "^3.0.4",
