    "parse-link-header": "^1.0.1"
  },
  "keywords": [
    "angular",
    "frrri",
    "ng-frrri",
    "ngxs",
    "api",
    "crud",
    "state management"
  ],
  "author": "marian@bitflut.com bitflut.com",
  "homepage": "https://bitflut.gitbook.io/ng-frrri/",
